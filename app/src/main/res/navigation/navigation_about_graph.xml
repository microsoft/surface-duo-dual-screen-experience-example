<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_about_graph"
    app:startDestination="@id/fragment_about_team">

    <fragment
        android:id="@+id/fragment_about_team"
        android:name="com.microsoft.device.display.sampleheroapp.presentation.about.fragments.AboutTeamFragment"
        android:label="AboutTeamFragment"
        tools:layout="@layout/fragment_about_team">

        <action android:id="@+id/action_about_team_to_licenses"
            app:destination="@id/fragment_about_licenses"
            app:launchScreen="end" />

    </fragment>

    <fragment
        android:id="@+id/fragment_about_licenses"
        android:name="com.microsoft.device.display.sampleheroapp.presentation.about.fragments.AboutLicensesFragment"
        android:label="AboutLicensesFragment"
        tools:layout="@layout/fragment_about_licenses" />

</navigation>